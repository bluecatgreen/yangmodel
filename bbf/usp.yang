            same as the interface object MACAddress.";
            type string {
              length "0..256";
            }
          }
          leaf MediaType {
            config false;
            description
            "Media type of this {{object}}. This is derived from
            the value of the intfType parameter defined in
            {{bibref|IEEE1905.1a|Tables 5.2: intfDescriptor
            elements}}. {{enum}}";
            type enumeration {
              enum "IEEE 802.3u" {
                description
                "IEEE 802.3u Fast Ethernet";
              }
              enum "IEEE 802.3ab" {
                description
                "IEEE 802.3ab Gigabit Ethernet";
              }
              enum "IEEE 802.11b" {
                description
                "IEEE 802.11b (2.4GHz)";
              }
              enum "IEEE 802.11g" {
                description
                "IEEE 802.11g (2.4GHz)";
              }
              enum "IEEE 802.11a" {
                description
                "IEEE 802.11a (5GHz)";
              }
              enum "IEEE 802.11n 2.4" {
                description
                "IEEE 802.11n (2.4GHz)";
              }
              enum "IEEE 802.11n 5.0" {
                description
                "IEEE 802.11n (5GHz)";
              }
              enum "IEEE 802.11ac" {
                description
                "IEEE 802.11ac (5GHz)";
              }
              enum "IEEE 802.11ad" {
                description
                "IEEE 802.11ad (60GHz)";
              }
              enum "IEEE 802.11af" {
                description
                "IEEE 802.11af";
              }
              enum "IEEE 1901 Wavelet" {
                description
                "IEEE 1901 Wavelet";
              }
