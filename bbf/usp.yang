          list Port {
            key "PortIDSubtype PortID";
            config false;
            description
            "This table contains information about ports on
            discovered LLDP devices.";
            leaf PortIDSubtype {
              config false;
              description
              "* ''8-255'' (Reserved)";
              type uint32 {
                range "min..255";
              }
