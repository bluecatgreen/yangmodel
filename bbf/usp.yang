              description
              "{{datatype|expand}}";
              type Alias;
            }
            leaf Tag {
              config true;
              description
              "Option tag (code) {{bibref|RFC3315|Section
              22.1}}.";
              type uint32 {
                range "0..65535";
              }
            }
            leaf Value {
              config true;
              description
