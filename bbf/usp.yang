            {{param|###.Bridging.ProviderBridge.{i}.SVLANcomponent}}.";
            type boolean;
            default "false";
          }
          leaf-list ServiceAccessPriorityTranslation {
            config true;
            description
            "The parameter is applicable to deployments of
            {{object|##.Bridge}} instances that are referenced
            by
            {{param|###.Bridging.ProviderBridge.{i}.SVLANcomponent}}.";
