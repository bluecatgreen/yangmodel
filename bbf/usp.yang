          enum "AAAA" {
            description
            "{{bibref|RFC3596}}";
          }
          enum "SRV" {
            description
            "{{bibref|RFC2782}}";
          }
          enum "PTR" {
            description
            "{{bibref|RFC1035}}";
          }
        }
      }
