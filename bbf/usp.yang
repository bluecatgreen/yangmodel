            description
            "The value extracted from the TTL field of the DNS
            SRV record {{bibref|RFC2782}}.";
            type uint32 {
              range "min..2147483647";
            }
          }
          leaf Priority {
            config false;
            description
            "The value extracted from the Priority field of the
