        list Service {
          key "InstanceName ApplicationProtocol TransportProtocol Domain";
          config false;
          description
          "*{{param|Target}} = \"ssh.example.com.\"";
          leaf InstanceName {
            config false;
            description
            "Human-readable instance name
            {{bibref|DNS-SD|Section 4.1.1 Instance Names}}
            (i.e.  The \"<{{param}}>\" portion of the
