            type enumeration {
              enum "Disabled";
              enum "Enabled";
              enum "Error";
            }
            default "Disabled";
          }
          leaf Alias {
            config true;
            description
            "{{datatype|expand}}";
            type Alias;
          }
          leaf DNSServer {
            config true;
            description
            "Note: {{param}} is only writable when
            {{param|Type}} is {{enum|Static|Type}}; otherwise,
            {{param}} is automatically configured as result of
            DHCP, IPCP, or RA received DNS server information.";
            type inet:ip-address;
          }
          leaf Interface {
            config true;
            description
            "Note: {{param}} is only writable when
            {{param|Type}} is {{enum|Static|Type}}; otherwise,
            {{param}} is automatically configured as result of
            DHCP, IPCP, or RA received DNS server information.";
