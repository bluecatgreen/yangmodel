            config true;
            description
            "Enables or disables this SentOption table entry.";
            type boolean;
            default "false";
          }
          leaf Alias {
            config true;
            description
            "{{datatype|expand}}";
            type Alias;
          }
          leaf Tag {
            config true;
            description
            "Option tag as defined in {{bibref|RFC2132}}.";
            type uint32 {
              range "1..254";
