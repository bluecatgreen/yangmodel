            leaf Key {
              config false;
              description
              "The key that identifies the text record";
              type string {
                length "0..255";
              }
            }
            leaf Value {
              config false;
              description
              "The value of the text record.";
              type string {
                length "0..255";
              }
            }
          }
        }
      }
    }
    container NAT {
      description
      "The entire {{object}} object only applies to IPv4.";
