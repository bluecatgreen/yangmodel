        }
        leaf SentOptionNumberOfEntries {
          config false;
          description
          "{{numentries}}";
          type uint32;
        }
        leaf ReqOptionNumberOfEntries {
          config false;
          description
          "{{numentries}}";
          type uint32;
        }
        list SentOption {
          key "Tag";
          unique "Alias";
